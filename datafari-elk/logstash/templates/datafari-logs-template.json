{
	"template" : "logs",
	"settings" : {
		"number_of_shards" : 1,
		"number_of_replicas" : 1
	},
	"mappings": {
		"_default_": {
			"_all" : {"enabled" : true},
			"dynamic" : "true",
			"properties": {
				"date": {
					"type": "date",
					"format": "yyyy-MM-dd HH:mm:ss"
				},
				"level": {
					"type": "string",
					"index": "not_analyzed"
				},
				"host": {
					"type": "string",
					"index": "not_analyzed"
				},
				"path": {
					"type": "string",
					"index": "not_analyzed"
				},
				"process": {
					"type": "string",
					"index": "not_analyzed"
				},
				"component": {
					"type": "string",
					"index": "not_analyzed"
				},
				"package": {
					"type": "string",
					"index": "not_analyzed"
				},
				"content": {
					"type": "string",
					"index": "not_analyzed"
				}
			}
		}
	}
}
