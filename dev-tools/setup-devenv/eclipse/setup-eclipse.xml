<?xml version="1.0" encoding="ISO-8859-1" ?>
<project name="Datafari-setup-eclipse" default="eclipse"
	basedir=".">

	<dirname property="project.dir" file="${ant.file.Datafari-setup-eclipse}" />

	<!-- Copy .project, .classpath and .settings directory to the root of the 
		Eclipse project directory -->
	<target name="eclipse">
		<copy todir="${project.dir}">
			<fileset dir="./conf" />
		</copy>
	</target>
</project>